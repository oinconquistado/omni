{
  "name": "omni-tenant-server",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch api/index.ts",
    "build": "pnpm clean && tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "lint": "biome lint .",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "@repo/server-core": "workspace:*",
    "@repo/shared-types-and-schemas": "workspace:*",
    "ioredis": "^5.7.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^22.15.3",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.2",
    "fastify": "^5.2.0",
    "prisma": "^6.16.1",
    "supertest": "^7.1.4",
    "tsx": "^4.0.0",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  }
}
